@{
    ViewData["Title"] = ViewBag.title;
    Layout = null;
}

<h2>@ViewBag.title</h2>
<div class="row">
    <div id="satButtons" class="col-sm-12 text-right">
    </div>
</div>
<div class="col-sm-12 text-center">
    <div id="satBadInputAlert" class="alert alert-danger fade show" style="display:none">
    </div>
    <div id="satSavedAlert" class="alert alert-success fade show" style="display:none">
    </div>
</div>
<table id="seasonalApportionmentTargetTable" class="display compact" style="width:100%">
    <thead>
        <tr>
            <th>Id</th>
            <th style="width: 50%">Apportionment Channel Name</th>
            <th style="width: 50%">Percent of Natural Flow (%)</th>
        </tr>
    </thead>    
</table>
<div class="row">
    <div id="satButtonsBottom" class="col-sm-12 text-right">

    </div>
</div>

<script>
    $.getScript("/js/Solver/SeasonalApportionmentTarget.js");
</script>