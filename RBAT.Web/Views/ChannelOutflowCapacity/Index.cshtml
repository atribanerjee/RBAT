@{
    Layout = null;
    ViewData["Title"] = ViewBag.title;
}

<div class="modal cocModal" id="cocModal" tabindex="-1" style="z-index : 1051">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title"><b>@ViewBag.title</b></h5>
                <button type="button" class="close" onclick="cocModalClose()" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">                
                <input type="hidden" id="cocElementID" value="@ViewBag.elementID" />
                <div class="row">
                    <div class="col-sm-6">
                        <label for="projectList">Channel: @ViewBag.elementName</label>
                    </div>
                    <div id="cocButtons" class="col-sm-6 text-right">
                    </div>
                </div>
                <div class="col-sm-12 text-center">
                    <div id="cocBadInputAlert" class="alert alert-danger fade show" style="display:none">
                    </div>
                    <div id="cocSavedAlert" class="alert alert-success fade show" style="display:none">
                    </div>
                </div>
                <table id="cocTable" class="display compact" style="width:100%">
                    <thead>
                        <tr>
                            <th>Id</th>
                            <th>@ViewBag.firstColumnName</th>
                            <th>Maximum Outflow (m3/s)</th>
                        </tr>
                    </thead>                  
                </table>
                <div class="row">
                    <div id="cocButtonsBottom" class="col-sm-12 text-right">

                    </div>
                </div>
                <div id="cocDetailShowModal"></div>
            </div>
            <div class="modal-footer">
                <button id="cocClose" type="button" class="btn btn-secondary" onclick="cocModalClose()">Close</button>
            </div>
        </div>
    </div>
</div>
<script src="@Url.Content("~/js/ChannelOutflowCapacity/ChannelOutflowCapacity.js")"></script>
